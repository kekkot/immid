    <nav class="nav-block">
        <div class="nav-info-block">
            <img class="nav-logo" src="~/image/Group 1569.png">
            <div class="nav-block-text">
                <div class=" dropdown">
                    <button class="dropbtn nav-links">Об
                                институте</button>
                    <div class="dropdown-content">
                        <a href="#">О нас</a>
                        <a href="#">Руководство</a>
                        <a href="#">Ученый совет</a>
                        <a href="#">Кафедры</a>
                        <a href="#">Комиссии</a>
