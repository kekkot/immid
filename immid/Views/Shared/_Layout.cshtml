<body class="page">
    <nav id="mySidenav" class="sidenav">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <button class="accordion">Наука</button>
        <div class="panel">
            <a class="link-panel">Журнал</a>
            <a class="link-panel">Конференции</a>
            <a class="link-panel">Спецсовет</a>
        </div>
        <button class="accordion">Проекты</button>
