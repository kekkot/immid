                <i class="social-icon__hover fa fa-vk "></i>
                <i class="social-icon__hover fa fa-facebook pt-2 pb-2"></i>
                <i class="social-icon__hover fa fa-instagram"></i>
            </div>
     </footer>
     <footer class="footer-block">
