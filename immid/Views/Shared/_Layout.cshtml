            <a class="link-panel">Гражданская оборона</a>
            <a class="link-panel">Противодействие
                терроризму</a>
        </div>
        <div class="contacts">
            <div class="contacts-col">
                <p class="text-contact">Контакты</p>
                <p class="subtext-contact">Пр-т Вернадского, д. 20, <b>Симферополь</b></p>
            </div>
            <img style="width:41px;height:41px;" src="~/image/add_location.png">
        </div>
